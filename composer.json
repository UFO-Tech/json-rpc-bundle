{
  "name": "ufo-tech/json-rpc-bundle",
  "description": "The bundle for easy using json-rpc api on your project",
  "version": "6.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Maystrenko",
      "email": "ashterix69@gmail.com"
    }
  ],
  "type": "symfony-bundle",
  "homepage": "https://github.com/ufo-tech/json-rpc-bundle",
  "keywords": [
    "ufo",
    "symfony",
    "api",
    "bundle",
    "json-rpc"
  ],
  "require": {
    "php": ">=8.2",
    "symfony/framework-bundle": ">=6.4",
    "symfony/serializer": ">=6.4",
    "psr/http-client": "*",
    "nyholm/psr7": "*",
    "ufo-tech/rpc-exceptions": ">=1.0.3",
    "symfony/console": ">=6.4",
    "symfony/http-kernel": ">=6.4",
    "symfony/validator": ">=6.4",
    "symfony/http-client": ">=6.4",
    "symfony/process": ">=6.4",
    "ext-dom": "*",
    "ufo-tech/rpc-objects": ">=2.0",
    "doctrine/persistence": ">=3.1",
    "symfony/security-bundle": ">=6.4",
    "phpdocumentor/reflection-docblock": ">=5.3",
    "symfony/expression-language": ">=6.3"
  },
  "autoload": {
    "psr-4": {
      "Ufo\\JsonRpcBundle\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "symfony": {
      "endpoint": [
        "https://api.github.com/repos/ufo-tech/recipes/contents/index.json?ref=main"
      ]
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
